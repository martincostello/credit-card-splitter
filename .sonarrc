{
    "browserslist": [],
    "connector": {
        "name": "chrome",
        "options": {
            "waitFor": 1000
        }
    },
    "formatter": "stylish",
    "ignoredUrls": {
        "bills.martincostello.com": [ ],
        "cdn.martincostello.com": [ "no-disallowed-headers", "strict-transport-security", "x-content-type-options" ],
        "cdnjs.cloudflare.com": [ "no-disallowed-headers", "strict-transport-security", "x-content-type-options" ],
        "fonts.googleapis.com": [ "no-disallowed-headers", "no-html-only-headers", "strict-transport-security", "x-content-type-options" ],
        "fonts.gstatic.com": [ "no-disallowed-headers", "no-html-only-headers", "strict-transport-security" ],
        "localhost": [ "content-type", "highest-available-document-mode", "no-disallowed-headers", "no-html-only-headers", "strict-transport-security", "x-content-type-options" ],
        "maxcdn.bootstrapcdn.com": [ "content-type", "no-disallowed-headers", "strict-transport-security", "x-content-type-options" ],
        "stats.g.doubleclick.net": [ "no-disallowed-headers" ],
        "www.google-analytics.com": [ "content-type", "no-disallowed-headers" ]
    },
    "rules": {
        "apple-touch-icons": "error",
        "axe": "error",
        "content-type": "error",
        "disown-opener": "error",
        "highest-available-document-mode": "error",
        "html-checker": "error",
        "manifest-app-name": "error",
        "manifest-exists": "error",
        "manifest-file-extension": "error",
        "manifest-is-valid": "error",
        "meta-charset-utf-8": "error",
        "meta-viewport": "error",
        "no-disallowed-headers": "error",
        "no-friendly-error-pages": "error",
        "no-html-only-headers": "error",
        "no-protocol-relative-urls": "error",
        "no-vulnerable-javascript-libraries": "error",
        "ssllabs": "off",
        "strict-transport-security": "error",
        "validate-set-cookie-header": "error",
        "x-content-type-options": "error"
    },
    "rulesTimeout": 120000
}
